		<h1>All Todo Lists</h1>
		{{#each users}}
			<h2>{{this.username}}</h2>
			<ul class="todoItems">
				{{#each this.items}}
					<li class="item {{#if this.deletedAt}}deleted{{/if}}">
            {{#if this.completed}}
                <span class='completed'>{{this.thing}}</span>
            {{else}}
                <span>{{this.thing}}</span>
            {{/if}}
					</li>
				{{/each}}
			</ul>
		{{/each}}

    <a href="/logout">Logout</a>
    <a href="/">Home</a>